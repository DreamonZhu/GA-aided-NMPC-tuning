    "libInc": [
      "${workspaceFolder}/lib/g-test/googletest/include",
      "${workspaceFolder}/lib/g-benchmark/include"
    ]
  },
  "configurations": [
    {
      "name": "GA aided NMPC tuning",
      "intelliSenseMode": "clang-x64",
